---
import Contact from "../components/Contact.astro";
import Footer from "../components/Footer.astro";
import Geodashboard from "../components/Geodashboard.astro";
import Hero from "../components/Hero.astro";
import Logbook from "../components/Logbook.astro";
import Service from "../components/Service.astro";
import Testimoni from "../components/Testimoni.astro";
import Works from "../components/Works.astro";
import Layout from "../layouts/Layout.astro";
---

<Layout theme="dark">
	<div class="typo-landing">
		<Hero />
		<Service />
		<Geodashboard />
		<Logbook />
		<Testimoni />
		<Works />
		<Contact />
		<Footer theme="dark" />
		<div class="w-full h-6"></div>
	</div>
</Layout>

<style is:global>
	.typo-landing h1,
	.typo-landing h2,
	.typo-landing h3,
	.typo-landing h4,
	.typo-landing h5,
	.typo-landing h6 {
		font-family: "Host-Grotesk", sans-serif;
	}
</style>
